{
	"obfuscation": {
		"classes": [
			{"id": "GuiScreen", "mcp": "net.minecraft.client.gui.GuiScreen", "obf": "axu"},
			{"id": "GuiContainerCreative", "mcp": "net.minecraft.client.gui.inventory.GuiContainerCreative", "obf": "ayu"},
			{"id": "EntityRenderer", "mcp": "net.minecraft.client.renderer.EntityRenderer", "obf": "bfk"},
			{"id": "ItemDash", "mcp": "mnm.mods.itemdash.LiteModItemDash"}
		],
		"methods": [
			{"id": "handleMouseInput", "mcp": "handleMouseInput", "srg": "func_146274_d", "obf": "k"},
			{"id": "handleKeyboardInput", "mcp": "handleKeyboardInput", "srg": "func_146282_l", "obf": "l"},
			{"id": "drawScreen", "mcp": "drawScreen", "srg": "func_73863_a", "obf": "a"},
			{"id": "updateCameraAndRender", "mcp": "updateCameraAndRender", "srg": "func_181560_a", "obf": "a"}
		]
	},
	"descriptors": [
		{"id": "handleMouseInput", "owner": "${GuiScreen}", "name": "${handleMouseInput}", "return": "VOID", "args": []},
		{"id": "handleMouseInputCreative", "owner": "${GuiContainerCreative}", "name": "${handleMouseInput}", "return": "VOID", "args": []},
		{"id": "handleKeyboardInput", "owner": "${GuiScreen}", "name": "${handleKeyboardInput}", "return": "VOID", "args": []},
		{"id": "drawScreen", "owner": "${GuiScreen}", "name": "${drawScreen}", "return": "VOID", "args": ["INT", "INT", "FLOAT"]},
		{"id": "updateCameraAndRender", "owner": "${EntityRenderer}", "name": "${updateCameraAndRender}", "return": "VOID", "args": ["FLOAT", "LONG"]},
		{"id": "onMouseInput", "owner": "${ItemDash}", "name": "handleMouseInput"},
		{"id": "onKeyboardInput", "owner": "${ItemDash}", "name": "handleKeyboardInput"},
		{"id": "onPreRenderScreen", "owner": "${ItemDash}", "name": "onPreRenderScreen"},
		{"id": "onPostRenderScreen", "owner": "${ItemDash}", "name": "onPostRenderScreen"}
	],
    "events": [
        {
            "name": "onHandleMouseInput",
            "cancellable": "true",
            "injections": [{
                "type": "HEAD",
                "method": "${handleMouseInput}"
            },
			{
				"type": "HEAD",
				"method": "${handleMouseInputCreative}"
			}],
            "listeners": ["${onMouseInput}"]
        },
		{
            "name": "onHandleKeyboardInput",
            "cancellable": "true",
            "injections": [{
                "type": "HEAD",
                "method": "${handleKeyboardInput}"
            }],
            "listeners": ["${onKeyboardInput}"]
        },
		{
			"name": "onPreRenderScreen",
			"injections": [{
				"type": "INVOKE",
				"method": "${updateCameraAndRender}",
				"target": "${drawScreen}"
			}],
			"listeners": ["${onPreRenderScreen}"]
		},
		{
			"name": "onPostRenderScreen",
			"injections": [{
				"type": "INVOKE",
				"method": "${updateCameraAndRender}",
				"target": "after(${drawScreen})"
			}],
			"listeners": ["${onPostRenderScreen}"]
		}
    ],
	"accessors": [
		"mnm.mods.itemdash.access.IGuiContainer",
		"mnm.mods.itemdash.access.IGuiCreativeContainer"
	]
}